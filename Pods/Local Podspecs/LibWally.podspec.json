{
  "name": "LibWally",
  "version": "0.0.1",
  "summary": "Swift wrapper for LibWally.",
  "description": "Swift wrapper for LibWally, a collection of useful primitives for cryptocurrency wallets.",
  "homepage": "https://github.com/blockchain/libwally-swift",
  "platforms": {
    "ios": "10"
  },
  "swift_versions": "5",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "authors": {
    "Sjors Provoost": "sjors@sprovoost.nl"
  },
  "source": {
    "git": "https://github.com/blockchain/libwally-swift.git",
    "tag": "0.0.1",
    "submodules": true
  },
  "source_files": "LibWally",
  "vendored_libraries": "CLibWally/libwally-core/src/.libs/libwallycore.a",
  "pod_target_xcconfig": {
    "SWIFT_WHOLE_MODULE_OPTIMIZATION": "YES",
    "SWIFT_INCLUDE_PATHS": "$(PODS_TARGET_SRCROOT)/CLibWally",
    "HEADER_SEARCH_PATHS": "$(PODS_TARGET_SRCROOT)/CLibWally/libwally-core/include",
    "LIBRARY_SEARCH_PATHS": "$(PODS_TARGET_SRCROOT)/CLibWally/libwally-core/src/.libs"
  },
  "preserve_paths": [
    "LibWally/LibWally.modulemap",
    "CLibWally"
  ],
  "module_map": "LibWally/LibWally.modulemap",
  "prepare_command": "./build-libwally.sh -sdc",
  "swift_version": "5"
}
